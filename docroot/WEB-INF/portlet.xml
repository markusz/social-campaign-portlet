			<mime-type>text/html</mime-type>
		</supports>
		<supported-locale>de</supported-locale>
		<supported-locale>en</supported-locale>
		<resource-bundle>de.tum.in.ziller.thesis.social_campaign.content.localization.Language-ext</resource-bundle>
		<portlet-info>
			<title>Social Campaign</title>
			<short-title>Social Campaign</short-title>
			<keywords>Social Campaign</keywords>
		</portlet-info>
		
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
